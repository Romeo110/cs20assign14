<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Results</title>
</head>
<body>
    <h1>Search Results</h1>
    <ul>
        <% results.forEach(result => { %>
            <li>
                <div><strong>Name:</strong> <%= result.companyName %></div>
                <div><strong>Ticker:</strong> <%= result.stockTicker %></div>
                <div><strong>Price:</strong> <%= result.latestPrice %></div>
            </li>
        <% }); %>
    </ul>
</body>
</html>
